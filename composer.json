{
    "name": "seo-toolkit/seo-toolkit",
    "type": "wordpress-plugin",
    "description": "SEO Toolkit is a smart plugin that assists you to optimize your website for purposes of SEO easily.",
    "version": "1.0.1",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Luis A. Ochoa",
            "role": "Developer"
        }
    ],
    "keywords": [
        "seo",
        "google",
        "bing",
        "xml-sitemaps",
        "performance",
        "wordpress"
    ],
    "require": {
        "php": ">=5.6.20",
        "ext-json": "*",
        "ext-dom": "*"
    },
    "autoload": {
        "psr-4": {
            "Toolkit\\": "seo-toolkit/src/"
        }
    },
    "config": {
        "vendor-dir": "seo-toolkit/vendor/"
    }
}
